{
	"name": "pokemon-countdown",
	"version": "0.0.1",
	"private": true,
	"repository": {
		"type": "git",
		"url": "https://github.com/novakedy/Pokemon-Countdown"
	},
	"scripts": {
		"android": "react-native run-android",
